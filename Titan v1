-------Scripts------
TitanFrame.Active = true
TitanFrame.Draggable = true
local isAuto = false
local remoterebirths=game.ReplicatedStorage.RemotesEvent.RebirthEvent
local strong:IntValue=game.Players.LocalPlayer:WaitForChild("leaderstats"):WaitForChild("Strong")
local function Rebirthgame()
	if isAuto == false then
		return
	end
	remoterebirths:FireServer()
end
AutoRebirthButton.Activated:Connect(function()
	if isAuto == false then
		isAuto = true
		succus.Size = UDim2.new(1,0,1,0)
		Rebirthgame()
	else
		isAuto = false
		succus.Size = UDim2.new(0,0,0,0)

	end
end)
strong.Changed:Connect(Rebirthgame)

local isAutolift = false
automuscleemp.MouseButton1Click:Connect(function()
	if isAutolift == true then
		isAutolift = false
		succus_2.Size = UDim2.new(0,0,0,0)
	else
		succus_2.Size = UDim2.new(1,0,1,0)

		isAutolift = true
	end
end)

local machineactive = game.ReplicatedStorage.RemotesEvent.MachineActiveFunction
local MachineActiveEvent = game.ReplicatedStorage.RemotesEvent.MachineActiveEvent
local isapin = false
local machineuse : ObjectValue = game.Players.LocalPlayer:WaitForChild("Machineuse")
local bechmemp = game.Workspace:WaitForChild("MachinesFolder"):WaitForChild("Bench Press Muscle Emperor")
local spinfu = game.ReplicatedStorage.RemotesEvent.SpinFunction
task.spawn(function()
	while true do
		task.wait(0.01)
		if isapin == true then
			spinfu:InvokeServer()
		end
		if isAutolift == false then
			task.wait(0.1)
			continue
		end
		if machineuse.Value == nil then
			task.wait(1)
			machineactive:InvokeServer(bechmemp,true)
			continue
		end
		MachineActiveEvent:FireServer()
	end
end)

autospinbutton.Activated:Connect(function()
	if isapin == false then
		succus_3.Size = UDim2.new(1,0,1,0)
		isapin = true
	else
		isapin = false
		succus_3.Size = UDim2.new(0,0,0,0)
	end
end)
